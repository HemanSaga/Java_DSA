import java.util.HashMap;
import java.util.HashSet;
import java.util.Map;
public class itinerary {
    public static String getStart(HashMap<String,String> tickets){
        HashMap<String,String> revMap = new HashMap<>();

        for(String key : tickets.keySet()){
            revMap.put(tickets.get(key),key);
        }
        for(String key : tickets.keySet()){
            if(!revMap.containsKey(key)) {
                return key;
            }
        }
        return null;

    }

    public static void main(String[] args){
        HashMap<String, String> itinerary = new HashMap<>();

        itinerary.put("Bangalore", "Ayodhya");
        itinerary.put("Mysore", "Ooty");
        itinerary.put("Ooty", "Coimbatore");
        itinerary.put("Coimbatore", "patna");
        itinerary.put("patna",   "Chennai");
        itinerary.put("Chennai", "Bangalore");

         String start = getStart(itinerary);

        while(itinerary.containsKey(start)){
            System.out.print(start+" - > ");
            start = itinerary.get(start);
        }
        System.out.print(start);
    }
}
